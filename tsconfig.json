{
  "extends": "@total-typescript/tsconfig/bundler/dom",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./app/*"]
    },
    "types": ["node"],
    "jsx": "preserve",
    "jsxImportSource": "react"
  },
  "include": [
    "app/**/*",
    "vite.config.ts",
    "react-router.config.ts"
  ],
  "exclude": [
    "node_modules",
    "build",
    "app/lib/agents-sdk"
  ]
}
